{% extends 'base.html.twig' %}

{% block title %}List of categories{% endblock %}

{% block body %}
    {% for category in categories %}
        <div class="container m-5">
            <p>
                Id : {{ category.id }} <br>
                Name : {{ category.name }}
            </p>
            <a href="{{ path('category_show', {categoryName: category.name}) }}">Voir programes</a>
        </div>
    {% else %}
    {% endfor %}
{% endblock %}
